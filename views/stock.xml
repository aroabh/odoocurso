<?xml version="1.0" encoding="UTF-8" ?>

<odoo>
    <record id="view_stock_tree" model="ir.ui.view">
        <field name="name">Vista de lista de Stock</field>
        <field name="model">reto.stock</field>
        <field name="arch" type="xml">
            <tree>
                <field name="prenda_id" />
                <field name="talla_id" />
                <field name="color_id" />
                <field name="cantidad_p" />
                <field name="id_proveedor" invisible="1"/>
            </tree>
        </field>
    </record>

     <record id="view_stock_form" model="ir.ui.view">
        <field name="name">Vista de formulario de Stock</field>
        <field name="model">reto.stock</field>
        <field name="arch" type="xml">
            <form>
                <group>
                     <field name="prenda_id" />
                </group>
                <group>
                     <field name="talla_id" />
                </group>
                <group>
                     <field name="color_id" />
                </group>
                <group>
                     <field name="cantidad_p" />
                </group>
                <group>
                     <field name="id_proveedor" invisible="1"/>
                </group>
            </form>
        </field>
    </record>
</odoo>